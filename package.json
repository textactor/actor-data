{
  "name": "@textactor/actor-data",
  "version": "0.1.1",
  "description": "Actors data module",
  "main": "./lib/index.js",
  "types": "./types/index.d.ts",
  "scripts": {
    "pretest": "npm run clean && npm run tsc",
    "test": "ava --no-cache --verbose lib/**/*.test.js",
    "posttest": "npm run clean-test-code",
    "tsc": "tsc",
    "tscw": "tsc -w",
    "preversion": "npm run tsc",
    "postversion": "git push --follow-tags",
    "prepublishOnly": "npm run tsc && npm run clean-test-code",
    "clean": "rimraf ./lib && rimraf ./types",
    "clean-test-code": "rimraf ./lib/**/*.test.js && rimraf ./types/**/*.test.d.ts",
    "dev": "yarn remove @textactor/domain && yarn remove @textactor/actor-domain && yarn link @textactor/domain && yarn link @textactor/actor-domain",
    "prod": "yarn unlink @textactor/domain && yarn unlink @textactor/actor-domain && yarn add @textactor/domain && yarn add @textactor/actor-domain"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/textactor/actor-data.git"
  },
  "keywords": [
    "actor",
    "data",
    "textactor"
  ],
  "author": "Dumitru Cantea",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/textactor/actor-data/issues"
  },
  "homepage": "https://github.com/textactor/actor-data#readme",
  "devDependencies": {
    "@types/joi": "^13.0.7",
    "@types/node": "latest",
    "ava": "^0.23.0",
    "rimraf": "^2.6.2",
    "typescript": "latest"
  },
  "dependencies": {
    "dynamodb": "^1.1.1",
    "joi": "^13.2.0"
  }
}
